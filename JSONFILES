      "cell_type": "code",
      "source": [
        "import requests\n",
        "import json\n",
        "import firebase_admin\n",
        "from firebase_admin import credentials, db\n",
        "# Step 1: Download the key from GitHub\n",
        "url = \"https://raw.githubusercontent.com/MDeluteAgrazamendez/sampleproject01/refs/heads/main/firebase_key.json\"\n",
        "response = requests.get(url)\n",
        "# Step 2: Save it locally in Colab\n",
        "with open(\"firebase_key.json\", \"wb\") as f:\n",
        "    f.write(response.content)\n",
        "# Step 3: Initialize Firebase\n",
        "# Check if the default Firebase app is already initialized\n",
        "if not firebase_admin._apps:\n",
        "    cred = credentials.Certificate(\"firebase_key.json\")\n",
        "    firebase_admin.initialize_app(cred, {\n",
        "        'databaseURL': 'https://grade8-json-db-73499-default-rtdb.asia-southeast1.firebasedatabase.app/'\n",
        "    })\n",
        "    print(\"Connected to Firebase!\")\n",
        "else:\n",
        "    print(\"Firebase app already initialized.\")\n",
        "#make sure the url is the raw file from the github"
      ],
